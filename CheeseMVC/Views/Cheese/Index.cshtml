@model IList<CheeseMVC.Models.Cheese>

   
    <table>
        <tr>
            <div class="row container ">
                <div class="col-2 bg-primary ">Name</div>


                <div class="col-2 bg-primary">Category</div>

            </div>
        </tr>
        <hr />
        @foreach (var cheese in Model)
        {
            <tr>

                <div class="row container">
                    <div class="col-2 bg-warning">@cheese.Name</div>
                    <div class="col-2 bg-warning">@cheese.Category.Name</div>

                </div>
                
            </tr>
        }
    </table>
